{
    "name": "@ianlucas/cslib",
    "version": "7.5.0",
    "description": "A TypeScript library for manipulating Counter-Strike-related data",
    "license": "MIT",
    "author": "Ian Lucas",
    "repository": "ianlucas/cslib",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "dump-default-graffiti-cdn": "tsx scripts/dump-default-graffiti-cdn.ts",
        "dump-default-graffiti": "tsx scripts/dump-default-graffiti.ts",
        "format": "prettier . --write",
        "generate": "tsx scripts/generate.ts > dist/generate-log.txt",
        "release": "npm run build && npm publish",
        "test": "jest"
    },
    "devDependencies": {
        "@types/jest": "^29.5.10",
        "@types/node": "^18.15.11",
        "@types/node-fetch": "^2.6.9",
        "dotenv": "^16.3.1",
        "jest": "^29.7.0",
        "node-fetch": "^2.7.0",
        "prettier": "3.1.0",
        "ts-jest": "^29.1.1",
        "tsx": "^4.6.1",
        "typescript": "^5.3.2"
    }
}
